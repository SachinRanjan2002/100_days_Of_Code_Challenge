//For Each loop in JAVA
package com.company;
class ForEach{
    void example(){
        int arr[]={12,13,14,44};
        int total=0;
        for(int i:arr){
            total=total+i;
            System.out.println(i);
        }

        System.out.println("Total: "+total);
    }
}
public class For_each {
    public static void main(String args[]){
       ForEach l=new ForEach();
       l.example();
    }
}
